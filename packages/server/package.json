{
  "name": "@ekoskola/server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon src/server.js --exec babel-node --presets node6",
    "format": "prettier --write \"src/**/*.js\"",
    "build": "babel src -d dist",
    "serve": "forever dist/server.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:rollback": "npx sequelize-cli db:migrate:undo",
    "db:undo": "npx sequelize-cli db:seed:undo",
    "db:seed": "npx sequelize-cli db:seed:all"
  },
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "babel-register": "^6.24.1",
    "bcrypt": "^5.1.0",
    "body-parser": "1.20.2",
    "caporal": "^1.3.0",
    "cli-table3": "^0.5.1",
    "colorette": "^1.1.0",
    "cookie-parser": "^1.4.4",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "forever": "^1.0.0",
    "formidable": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.4",
    "nodemon": "2.0.22",
    "pg": "8.10.0",
    "sequelize": "6.31.0",
    "umzug": "3.2.1",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  }
}
